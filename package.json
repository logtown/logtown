{
  "name": "logtown",
  "version": "2.1.3",
  "description": "Simple Logging Facade for JavaScript",
  "main": "es6/common/index.js",
  "module": "index.js",
  "scripts": {
    "test": "npm run build && ava --verbose",
    "es5-umd": "rollup -c rollup.config.browser.es5.js",
    "es5-commonjs": "rollup -c rollup.config.cjs.es5.js",
    "es6-commonjs": "rollup -c rollup.config.cjs.es6.js",
    "build": "npm run es5-umd && npm run es5-commonjs && npm run es6-commonjs"
  },
  "keywords": [
    "logtown",
    "logger",
    "log",
    "logging facade"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/logtown/logtown"
  },
  "author": "Dmitry Zelenetskiy <dmitry.zelenetskiy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/logtown/logtown/issues"
  },
  "homepage": "https://github.com/logtown/logtown",
  "dependencies": {
    "ember-empty-object": "^1.0.1",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "pad": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "@babel/core": "^7.0.0-beta.33",
    "babel-plugin-external-helpers": "^7.0.0-beta.3",
    "babel-preset-es2015": "^7.0.0-beta.3",
    "rollup": "^0.59.0",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
